/* Collection page image carousel */

.relative { overflow:hidden; text-align:center; } 
.relative img { display:none; margin:0; vertical-align:top; }
.relative img.first { display:inline; }

.footer .container { border:solid #f3f0e5; border-width:1px 0 0; }

#collectionPage .thumbnail { width:49%; }
#collectionPage .thumbnail.alpha { float:left; }
#collectionPage .thumbnail.omega { float:right; }
 
#categorydescription p { padding:10px 0; }

/* Refine Bar */

.container .refinebar.four.columns { margin:0 3% 0 0; position:relative; } 

.refinebar h5 { margin:0; padding:0; font-size:14px; }
.refinebar .refine-list { margin:0 0 20px; display:none; }
.refinebar .refine-list-wrapper { padding:10px; border:solid #f3f0e5; border-width:1px; }

.refinebar .refine-list-wrapper div { padding:0; margin:0; max-height:245px; overflow-y:scroll; overflow-x:hidden; }
.refinebar .refine-list-wrapper div div { overflow:hidden; margin:0; padding:2px 0; }
.refinebar .refine-list-wrapper div div a { float:left; }
.refinebar .refine-list-wrapper div div input[type="checkbox"] { float:right; }

.ie8 .refinebar .refine-list-wrapper div { min-height:auto; }

.refinebar .filter-helper { position:absolute; left:40%; background-color:#404040; padding:5px; -moz-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px; }
.refinebar .filter-helper a { color:#fff; }

.filter-helper-back { position:fixed; left:50%; margin:0 0 0 -50px; bottom:20px; background-color:#404040; padding:5px; -moz-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px; }
.filter-helper-back a { color:#fff; }

/* Breadcrumb */

.breadcrumb { color:#404040; } 

/* Forms */

.form-wrapper { width:100%; }
.form-wrapper form { padding:0 0 200px; }
.form-wrapper h4 { border-width:0; font-size:13px; margin:0; }

/* Account Summary */

#accountSummary h1 { border-width:0; margin:0 0 5px; }
#accountSummary h1 span { display:block; font-size:13px; text-transform:capitalize; }
#accountSummary h2 { text-align:center; text-decoration:underline; font-size:16px; margin:30px 0 15px; }

#accountSummary .page-header { text-align:center; border:solid #f3f0e5; border-width:0 0 1px; }

#accountSummary .sixteen.acc-address-book { display:none; }

#customer_orders table thead tr th { text-decoration:underline; } 
#customer_orders table tbody tr td { text-align:center; } 

.account-navigation { margin:0 0 10px; }
.account-navigation ul { list-style:none; margin:0; padding:0; overflow:hidden; display:inline-block; }
.account-navigation ul li { float:left; padding:0 10px; margin:0; }
.account-navigation a.button { font-size:13px; background-color:#404040; font-family:Arial, Sans-serif; }

h1 a.logout { color:#404040; }

#cart ul li.loader { min-height:136px; background-color:#fff; background-image:url("{{ 'ajax-loader.gif' | asset_url }}"); background-position:200px center; background-repeat:no-repeat; }
#cart ul li.loader a { display:none; }

.cart-hidden { display:none!important; }

/* Product Page */

.flexslider .loader { width:100%; height:100%; position:absolute; z-index:100; background-color:#fff; background-image:url("{{ 'ajax-loader.gif' | asset_url }}"); background-position:center center; background-repeat:no-repeat; }

.mm-menu a.close span { width:40px; height:41px; background-color:#404040; position:absolute; left:15px; text-indent:0; color:#fff; top:10px; text-align:center; font-size:30px; line-height:40px; }

.size-chart { margin:0 0 10px; }
.size-chart a { font-size:12px; text-decoration:underline; }

.single-option-selector { margin:0 0 10px; }

.product-listing { padding:0 0 50px; }

h1.product_name { text-align:center; }

div.v-error { color:#f00; font-size:13px; font-family:Arial, Sans-serif; }
select.v-error { background-color:#ffcccc; margin:0 0 5px; }

/* No Javascript */

.nav .no-js-hidden { display:none; }
.no-js .icon-cart { display:none; }
.no-js .no-js-hidden { display:block; }

/* Size Charts */

#inlineChart { padding:10px; }
#inlineChart h2 { margin:0; }
#inlineChart p { margin:0 0 10px; }
#inlineChart p.fit-title { margin:10px 0 0; }

#inlineChart,
#inlineChart .tees,
#inlineChart .blazer,
#inlineChart .outerwear,
#inlineChart .polo,
#inlineChart .jean,
#inlineChart .pant,
#inlineChart .shirt,
#inlineChart .shoe,
#inlineChart .short,
#inlineChart .knit { display:none; }

#inlineChart.tees .tees,
#inlineChart.blazer .blazer,
#inlineChart.outerwear .outerwear,
#inlineChart.polo .polo,
#inlineChart.jean .jean,
#inlineChart.pant .pant,
#inlineChart.shirt .shirt,
#inlineChart.shoe .shoe,
#inlineChart.short .short,
#inlineChart.knit .knit { display:block; } 

#inlineChart table { overflow:hidden; margin:10px 0 0; width:100%; border-spacing:1px; border-collapse:inherit; }
#inlineChart table tbody tr td,
#inlineChart table thead tr td { width:33%; text-align:center; font-size:10px; padding:0; }
#inlineChart .shoe table thead tr td ,
#inlineChart .shoe table tbody tr td { width:25%; }
#inlineChart .jean table thead tr td,
#inlineChart .jean table tbody tr td,
#inlineChart .pant table thead tr td,
#inlineChart .pant table tbody tr td,
#inlineChart .shirt table thead tr td,
#inlineChart .shirt table tbody tr td { width:14%; }
#inlineChart table tbody tr td.first,
#inlineChart table thead tr td { background-color:#f3f0e5; }
#inlineChart table tbody tr td { background-color:#ededed; } 

/* Cart Page */

.cart h1.collection_title { margin:0 auto; display:block; line-height:38px; text-align:center; }

.cart .continue_shopping { text-align:center; float:none; }

.cart .cart_items h4 { font-size:15px; text-align:center; }

.cart .cart_items .alpha { width:40%; float:left; }
.cart .cart_items .omega { width:55%; float:left; margin:0 0 0 5%; }
.cart .cart_items .columns a img { vertical-align:top; } 

.cart .cart-subtotal h4 { text-align:center; font-size:15px; }







