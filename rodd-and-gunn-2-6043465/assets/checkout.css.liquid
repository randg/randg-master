body { min-width:768px; font-family:Arial, Sans-serif; color:#404040; }

#container { color:#404040; padding:0 20px; position:relative; }
#logo { display:block; padding:0; margin:10px auto; width:175px; height:47px; background-image:url('{{ "logo.png" | asset_url }}'); background-repeat:no-repeat; background-position:center center; background-size:100%; }
#tagline { display:none; }
#products { border-right:solid 1px #f3efdf; }
#products h2 { display:none; }
#overview { padding:0; width:50%; float:left; margin:41px 0 0; }
#content { width:47%; float:right; margin:60px 0 0; }
#footer { clear:both; padding:20px 10px; border:solid #f3efdf; border-width:1px 0 0; }
#footer p { text-align:center; color:#404040; font-family:Arial, Sans-serif; font-size:11px; }
#steps { text-align:center; font-family:"{{ settings.nav_google_webfont_font | replace: '+', ' ' }}"!important; font-size:15px; text-transform:capitalize; padding:10px 0; position:absolute; top:55px; left:20px; right:20px; margin:0; background-color:#f3efdf; }
#main { overflow:hidden; padding:0 0 120px!important; }
#buttons { position:absolute; left:50%; width:500px; margin:0 0 0 -250px!important; bottom:80px; }
#addresses table { width:100%; }
.step2 #commit-button { margin:0 15px 0 0; }
#customer-login-link,
#buttons input, 
#buttons a,
input.submit-btn { 
    background: {{ settings.btn_color }};
    color: {{ settings.btn_text_color }};
    border: 0;
    padding: 7px 20px;
    text-align: center;
    cursor: pointer;
    {% if settings.nav_typeface_method == "google_webfonts" %}
      font-family: "{{ settings.nav_google_webfont_font | replace: '+', ' ' }}";
    {% else %}
      font-family: {{ settings.nav_websafe_font | replace: '+', ' '  }};
    {% endif %}  
    font-weight: {{ settings.button_weight }};      
    font-size: {{ settings.button_font_size }}px;
    text-transform: {{ settings.button_font_style }};
    display: inline-block;
    -webkit-transition: all 200ms ease 0s;
    -moz-transition: all 200ms ease 0s;
    -ms-transition: all 200ms ease 0s;
    -o-transition: all 200ms ease 0s;
    transition: all 200ms ease 0s;
    -webkit-appearance: none;
    -webkit-font-smoothing: antialiased;
    font-smoothing: antialiased;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -moz-border-radius: 3px;
     -webkit-border-radius: 3px;
     border-radius: 3px; 
 } 

#buttons a { background:#ede9dd; color:#404040; text-decoration:none; margin:0 0 0 15px; }
#customer-login-link { margin:0 0 0 10px; text-decoration:none; padding:7px 10px!important; }

.error-message { color:#f00!important; text-transform:capitalize; font-style:13px!important; }




.step2 #buttons { width:625px!important; margin:0 0 0 -312px!important; }


table#thumbs { width:100%; font-size:14px; }
#thumbs div { float:left; margin:0 10px 0 0; }
#thumbs td { width:100%; clear:both; margin:5px 0 0; padding:0 0 5px; overflow:hidden; border:solid #f3efdf; border-width:0 0 1px; }
#thumbs h3,
#thumbs span { color:#404040; padding:0 0 3px; }
#thumbs span.plist-line-price { display:block; padding:3px 0 0; }
#wallet { padding:0 0 5px; }
#cost { color:#404040; font-weight:bold; font-size:20px; text-align:center; background-color:#f3efdf; padding:15px 0; }
#shipping-rates { margin:0 0 20px; }

.group p { font-size:13px!important; }
.sst { margin:15px 0 0!important; font-size:13px; }

#email { color:#404040; padding:0 0 15px; margin:0 0 15px; border:solid #f3efdf; border-width:0 0 1px; }
#email label { margin:0 0 10px; font-family:"{{ settings.nav_google_webfont_font | replace: '+', ' ' }}"!important; font-size:20px!important; }
#email .sign-in-message { margin:10px 0 0; display:block; font-size:13px; }
#order_email { border:solid 1px #cccccc; padding:5px 0!important; text-indent:10px; }
table#billing { border-bottom:solid 1px #f3efdf; padding:0 0 10px; }
table#billing td { float:right; font-size:13px; width:100%!important; max-width:75%!important; }
table#billing td.lbl { width:20% !important; float:left; }
table#billing td input { width:99% !important; border:1px solid #ccc; padding:5px 0; text-indent:10px; margin:0 0 10px; }
table#billing td select { margin:0 0 10px; width:100% !important; } 
table#billing .multiple_address_selection td { width:auto !important; margin:0 0 10px; max-width:100% !important; float:none; }
table#billing .multiple_address_selection td select { width:100% !important; padding:0 5%; display:block; }
table#billing #billing-is-shipping td input { float:left; width:auto!important; }
table#billing #billing-is-shipping td label { float:left; margin:0 0 0 10px; width:80%; }

table#shipping { margin:0; }
table#shipping td { float:right; font-size:13px; width:100%!important; max-width:75%!important; }
table#shipping td.lbl { width:20% !important; float:left; }
table#shipping td input { width:99% !important; border:1px solid #ccc; padding:5px 0; text-indent:10px; margin:0 0 10px; }
table#shipping td select { margin:0 0 10px; width:100% !important; } 
table#shipping .multiple_address_selection td { width:auto !important; margin:0 0 10px; max-width:100% !important; float:none; }
table#shipping .multiple_address_selection td select { width:100% !important; padding:0 5%; display:block; }

#addresses h3 { border-width:0; font-size:20px !important; color:#483F17; margin:0 0 10px; font-family:"{{ settings.nav_google_webfont_font | replace: '+', ' ' }}"!important; }
.address-notification span { font-size:13px; }
.gright { margin:20px 0 0; }

#buttons { margin:20px 0; }

.group h3 { font-size:18px !important; border:solid #f3efdf; border-width:0 0 1px; background:transparent; {% if settings.headline_typeface_method == "google_webfonts" %}
      font-family: "{{ settings.headline_google_webfont_font | replace: '+', ' ' }}";
    {% else %}
      font-family: {{ settings.headline_websafe_font | replace: '+', ' ' }};
    {% endif %}  
    font-weight: {{ settings.headline_weight }};    
    text-transform: {{ settings.headline_font_style}};
    color: #404040;
    display:block;
    letter-spacing: {{ settings.headline_letter_spacing }}px;
    text-rendering: optimizeLegibility;
    padding:0;
}

@media only screen and (min-width: 1200px) { 
    body { max-width:1200px; margin:0 auto; }
}

@media all and (min-width: 1240px) {
  #container { display:block; padding:0; }
  #steps { left:0; right:0; }
}
















