$(function() {
  $(".thumbnail img, .product_slider img, .cart_page_image img").unveil(200);

  $('#homepage_slider').flexslider({
    touch: false, 
    {% if settings.video_embed != blank %}
      pauseOnHover: true,
      video: true,
    {% endif %}
    controlNav: true,
    {% if settings.slideshow_arrows %} 
      directionNav: true, 
    {% else %} 
      directionNav: false, 
    {% endif %}
    animation: "{{ settings.slideshow_animation }}", 
    slideshowSpeed: {% if settings.slideshow_speed != blank %}{{ settings.slideshow_speed }}{% else %}10{% endif %}*1000 
  });

  $('.slider').flexslider({touch: false, controlNav: false, animation: "{{ settings.slideshow_animation }}" });
  
  $('.slider-no-nav').flexslider({touch: false, controlNav: false, directionNav: false, animation: "{{ settings.slideshow_animation }}" });

  $('.slider-no-nav-quick').flexslider({touch: false, controlNav: false, directionNav: false, animation: "{{ settings.slideshow_animation }}", slideshowSpeed: 4000 });

  $('.product_slider').flexslider({
    touch: false,
    pauseOnHover: true,
    controlNav: "thumbnails",
    directionNav: true,
    slideshow: false
  });

  var $header = $(".header");
  {% if settings.use_logo_home %}
    if ($(window).width() >= 768) {
      $('.logo img', $header).attr('src', $('.logo img').data('src-home'));
    }
  {% endif %}
  $(window).scroll(function() {
    if(!$('html').hasClass('mm-opened')) {
      $header.toggleClass('header_bar', $(window).scrollTop() > 57);
      if ($(window).scrollTop() > 57 && $(window).width() >= 768) {
        $('.mini-logo').show();
      } else {
        $('.mini-logo').hide();
      };
      if ($(window).scrollTop() > 0 && $(window).scrollTop() < 57 && $(window).width() >= 768) {
        $('.utility-navigation-wrap').css({ top: 8 + $(window).scrollTop() + 'px' });
      } else if ($(window).scrollTop() >= 57) {
        $('.utility-navigation-wrap').css({ top: 64 + 'px' });
      } else {
        $('.utility-navigation-wrap').css({ top: 8 + 'px' });
      };
      {% if settings.use_logo_home %}
        if($(window).width() >= 768) {
          if($(window).scrollTop() > 0) {
            $('.logo img', $header).attr('src', $('.logo img').data('src'));
          } else {
            $('.logo img', $header).attr('src', $('.logo img').data('src-home'));
          }
        }
      {% endif %}
    }
  });

  // Initiate the Mega Menu
  $('#nav').mmenu({         
    classes: 'mm-light',
    searchfield: true
  });







  // Add to Cart API Call
  $(document).on('submit', '.product_form', function(e) {
    e.preventDefault();
    Shopify.addItemFromForm($(this).attr('id'));
  });

  // Add to Cart API Callback
  Shopify.onItemAdded = function(line_item) {
    $('#cart').mmenu().trigger("open").on("opened.mm", function() {
      //alert('Woot');
    });
  };



  Shopify.updateCartInfo = function(cart, cart_summary_id, cart_count_id) {
  
  // Update cart count.
  if ((typeof cart_count_id) === 'string') {
    if (cart.item_count == 0) { 
      jQuery('#' + cart_count_id).html('0 items'); 
    }
    else if (cart.item_count == 1) {
      jQuery('#' + cart_count_id).html('1 item' + ' (' + cart.total_price + ')');
    }
    else {
      jQuery('#' + cart_count_id).html(cart.item_count + ' items' + ' (' + cart.total_price + ')');
    }
  } 
};

Shopify.onCartUpdate = function(cart) {
  Shopify.updateCartInfo(cart, 'cart-info', 'shopping-cart');
};










  if ($('html').hasClass('ie')) {
    $('.cart-button').click(function() { window.location = '/cart'; });
  } else {
    $('#cart').mmenu({
      position: 'right',
      classes: 'mm-light'
    });
    $('.icon-cart, .icon-menu').on("click", function(e) { 
      scrollTo(0,0); 
      $('#search').hide();
      e.stopPropagation();
      return false;
    });
  }

  $('#search-toggle, .search-close').on('click', function(e) {
    $('.search-close').css('padding-top', $('.header').height() + 100 + 'px')
    if($('#search').is(":visible") && $(window).scrollTop() == 0) {
      $(".index .header").removeClass('header_bar');
      $('.index .header .logo img').attr('src', $('.logo img').data('src-home'));
    } else {
      $(".index .header").addClass('header_bar');
      $('.index .header .logo img').attr('src', $('.logo img').data('src'));
    }

    $('#search').fadeToggle('fast').find('input').focus();
    e.stopPropagation();
    return false;
  })

  $('.lightbox').fancybox();
  
  if ($(window).width() >= 959) {
    $('.thumbnail').hover(function(){ $('.quick_shop', this).show(); }, function(){ $('.quick_shop', this).hide(); })
    var modal_width = '860px';
    if($(window).width() >= 1200 || $('html').hasClass('ie')) {
      modal_width = '1100px'
    }
    $(".quick_shop").fancybox({
      width: modal_width,
      height: 'auto',
      autoSize: false,
      padding: 0,
      beforeShow: function(e) {
        var $gallery = $('#' + $(this.element).data('gallery'));
        var $product = $(this.element).data("fancybox-href");        
        $("img", $gallery).unveil();
        $gallery.flexslider({
          touch: false,
          pauseOnHover: true,
          controlNav: "thumbnails",
          directionNav: false,
          {% if settings.product_slideshow_animation == 'none' or settings.product_slideshow_animation == 'zoom' %}
            slideshow: false,
          {% else %}
            animation: "{{ settings.product_slideshow_animation }}",
          {% endif %}
          slideshowSpeed: {% if settings.slideshow_speed != blank %}{{ settings.slideshow_speed }}{% else %}10{% endif %}*1000 
        });
        $('.flex-control-thumbs img', $product).click(function(e) { 
          var optionValue = $(this).attr('alt');
          var $selector = $('select.single-option-selector:contains(' + optionValue + ')', $product);
          if ($selector.length && $selector.val() != optionValue) {
            $selector.val(optionValue).trigger('change');
          }
        });
      }
    });
  } 

  if ($(window).width() >= 768) {
    $(".fancybox").fancybox({padding: 0, wrapCSS: 'gallery'});
    {% if settings.product_slideshow_animation == 'zoom' %}
      CloudZoom.quickStart();
    {% endif %}
  }

  $('.flex-control-thumbs img').click(function(e) { 
    var optionValue = $(this).attr('alt');
    var $selector = $('select.single-option-selector:contains(' + optionValue + ')');
    if ($selector.length && $selector.val() != optionValue) {
      $selector.val(optionValue).trigger('change');
    }
  });

  $('#tag_filter').change(function() { window.location = $('#tag_filter option:selected').val(); });  
  $("#cart_form input[type='number']").change(function() {$("#cart_form").submit(); });
  
	var tabs = $('ul.tabs');
	tabs.each(function(i) {
		var tab = $(this).find('> li > a');
		tab.click(function(e) {
			var contentLocation = $(this).attr('href');
			if(contentLocation.charAt(0)=="#") {
				e.preventDefault();
				tab.removeClass('active');
				$(this).addClass('active');
				$(contentLocation).show().css({'display': 'block'}).addClass('active').siblings().hide().removeClass('active');
			}
		});
	});

  $('.toggle').click(function() {
    if ($(window).width() <= 767) { 
      var $toggle = $(this);
      if($toggle.next('ul').is(':visible')) {
        $toggle.next('ul').hide();
        $toggle.children('span').html('+');
      } else {
        $toggle.next('ul').show();
        $toggle.children('span').html('-');
      }
    }
  });


});

